@model  IEnumerable<MyVitarak.Models.NotificationDetails>
 
   
@if (Model.Count() != 0)
{
    foreach (var item in Model)
    {
        <div class="col-md-12">
            <div class="ibox float-e-margins">

                <div>



                    <div class="ibox-content profile-content">
                        <h4><strong>
                            @item.MessegeFrom
                        <p><i class="fa fa-map-marker"></i> 
                            <small>@item.Address |  @item.Mobile </small></p>
                    </strong>
                        </h4>
                        
                        <p class="font12">
                           @item.Messege 
                        </p>
                        
                        <div class="user-button">
                            <div class="row">
                                <div class="col-md-6">
                                    <button type="button" id="btnsave" class="btn btn-primary btn-sm btn-block"><i class="fa fa-envelope"></i> Accept</button>
                                </div>
                                <div class="col-md-6">
                                    <button type="button" id="btnReject" class="btn btn-default btn-sm btn-block"><i class="fa fa-coffee"></i> Reject</button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    }
}


        <script>

            $('.accept').click(function () {
                var idValue = $(this).attr('id');
                if (confirm("Are you sure?")) {
                    $.ajax({
                        type: 'POST',
                        dataType: 'json',
                        url: '/Master/AddVendor',

                        data:
                        {
                            NotificationId: idValue

                        },
                        success: function (da) {
                            alert(da);
                            $('#btnsave1').show();
                            $('#btnUseThis').hide();
                        },

                        error: function (XMLHttpRequest, textStatus, errorThrown) {

                        }
                    });
                }
            })


        </script>