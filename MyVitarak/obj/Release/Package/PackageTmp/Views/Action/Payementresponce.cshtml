    function SaveTenant() {
       
        var url = "/Master/Dashboard";
        $.ajax({
            type: 'POST',
            dataType: 'json',
            url: '/Home/insert_tenant',

            data:
            {
                Name: $("#CName").text(),
                Address: $("#Address").text(),
                Mobile: $("#Mobile").text(),
                ContactPerson: $("#ContactPerson").text(),
                DbName: $("#BusinessName").text(),
                PlanID: $("#PlanID").text(),
                PaidAmount: $("#PaidAmount").text()

            },
            success: function (da) {
                alert(da);     
                window.location.href = url;
            },

            error: function (XMLHttpRequest, textStatus, errorThrown) {

                alert(textStatus);
            }
        });

        return false;
    }
  
